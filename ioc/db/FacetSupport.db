record(bo, "F2:WATCHER:FACETSUPPORT_STAT") {
  field(DESC, "Gun Watcher Running Status")
  field(SCAN, "Passive")
  field(PINI, "YES")
  field(DTYP, "Soft Channel")
  field(VAL, "0")
  field(OMSL, "supervisory")
  field(HIGH, "$(hightime)")
  field(ZNAM, "OFF")
  field(ONAM, "RUNNING")
}

record(bo, "F2:WATCHER:KEEPALIVE") {
  field(OUT, "F2:WATCHER:FACETSUPPORT_STAT PP")
  field(VAL, "1")
  field(PINI, "YES")
  field(FLNK, "F2:WATCHER:FACETSUPPORT_STAT")
  field(SCAN, "1 second")
}

